{
  "name": "@ryanar/hooks",
  "version": "0.1.0",
  "description": "React Hooks for general use",
  "main": "dist/hooks.cjs.js",
  "module": "dist/hooks.esm.js",
  "browser": "dist/hooks.umd.js",
  "types": "dist/index.d.ts",
  "typings": "lib/index.d.ts",
  "repository": "https://github.com/audiolion/hooks",
  "author": "Ryan Castner <castner.rr@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/audiolion/hooks/issues"
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --collectCoverage",
    "test:cov": "jest --watchAll --collectCoverage --coverageReporters=html"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^24.0.13",
    "@types/react": "^16.8.18",
    "@types/react-dom": "^16.8.4",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "babel-eslint": "9.x",
    "eslint": "5.x",
    "eslint-config-react-app": "^4.0.1",
    "eslint-plugin-flowtype": "2.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-react": "7.x",
    "eslint-plugin-react-hooks": "1.6.0",
    "jest": "^24.8.0",
    "jest-dom": "^3.4.0",
    "jest-junit": "^6.4.0",
    "prettier": "^1.17.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hooks-testing-library": "^0.5.0",
    "react-test-renderer": "16.8.6",
    "rollup": "^1.12.4",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.2.0",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "prettier": {
    "semi": true,
    "trailingComma": "all",
    "printWidth": 80,
    "singleQuote": true
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/index.ts",
      "!src/types/",
      "!<rootDir>/node_modules/"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true
        }
      }
    },
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.ts"
    ]
  }
}
